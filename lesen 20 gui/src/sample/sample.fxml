<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.CheckBox?>
<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <TextField fx:id="input" GridPane.columnIndex="0" GridPane.rowIndex="0" disable="false" onKeyReleased="#vis"></TextField>
    <Button fx:id="HELO" text=" say helo"  GridPane.rowIndex="2" GridPane.columnIndex="0" disable="true" onAction="#print"></Button>
    <Button fx:id="by" text=" say by"  GridPane.rowIndex="2" GridPane.columnIndex="1" disable="true" onAction="#print"></Button>
    <CheckBox fx:id="chc" GridPane.columnIndex="1" GridPane.rowIndex="0" onAction="#delet"></CheckBox>
    <Label fx:id="l" text="label" GridPane.rowIndex="3" GridPane.columnIndex="0"></Label>



</GridPane>